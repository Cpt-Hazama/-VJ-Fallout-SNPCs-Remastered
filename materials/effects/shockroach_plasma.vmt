//"UnlitTwoTexture_DX6"
"VertexLitGeneric"
{
 	"$model" 1
    "$nocull" "1"
	"$basetexture" "effects/shockroach_plasma"
	"$texture2" "effects/shockroach_plasma"

	"$translucent" "1"
	"$alpha" "0"
	"$additive" "1"

	// -- Not supported by VertexLitGeneric ?
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 1.5
			"texturescrollangle" 90
		}
		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 3
			"texturescrollangle" 150
		}
	}
}
